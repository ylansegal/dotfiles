  "atom-lint": {}
  "atom-terminal-panel": {}
  autocomplete:
    includeCompletionsFromAllBuffers: true
  "autocomplete-plus":
    enableAutoActivation: false
    includeCompletionsFromAllBuffers: true
  "autocomplete-ruby": {}
  awiki:
    wikiLocation: "/Users/ylansegal/iCloud/Wiki/"
  "bracket-matcher":
    highlightMatchingLineNumber: true
  core:
    closeDeletedFileTabs: true
    disabledPackages: [
      "policeman-rubocop"
      "autocomplete-paths"
      "autocomplete-snippets"
      "rspec-snippets"
      "bookmarks"
      "fancy-new-file"
      "atom-lint"
      "pretty-json"
      "linter-crystal"
      "linter-elixir-credo"
    ]
    ignoredNames: [
      ".git"
      ".DS_Store"
      "Thumbs.db"
      ".zeus.sock"
    ]
    packagesWithKeymapsDisabled: [
      "zentabs"
      "atom-elixir-formatter"
    ]
    projectHome: "/Users/ylansegal/mellmo-git"
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "limited"
    themes: [
      "unity-ui"
      "base16-ocean-dark-syntax-theme"
    ]
  editor:
    confirmCheckoutHeadRevision: false
    fontFamily: "Hack"
    fontSize: 15
    invisibles: {}
