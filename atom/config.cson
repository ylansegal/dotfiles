    plugins:
      "git-diff": true
  "one-light-ui": {}
  "rails-rspec": {}
  "release-notes":
    viewedVersion: "0.95.0"
  rspec:
    command: "smart_rspec"
    force_colored_results: false
  "spell-check":
    locales: [
      "en-US"
      "sp-MX"
    ]
  "split-diff":
